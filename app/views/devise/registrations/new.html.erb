    <div class="col-md-9">
    </div><!--fin col1-->

    <div class="col-md-3">
      <ul class="masthead-nav">
        <li><a><%= link_to "Home", root_path%></a></li>
        <li class="active"><a><%= link_to "Registrese ", new_user_registration_path %></a></li>
