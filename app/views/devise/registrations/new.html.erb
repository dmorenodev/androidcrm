<div class="container">
  <!------------------------------------------  Panel banner--------------------------->
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

   <div class="row">
     <div class="col-md-4">
       <p>Informac√≠on personal</p>
            <p><%= f.label :name %><br />
            <%= f.text_field :name %></p>

           <div><%= f.label :telefono %><br />
           <%= f.text_field :telefono %></div>

           <div><%= f.label :movil %><br />
           <%= f.text_field :movil %></div>

           <div><%= f.label :fecha_nacimiento %><br />
           <%= f.date_select :fecha_nacimiento %></div>
     </div>

     <div class="col-md-4">
       <p>Datos de Interes</p>
           <div><%= f.label :empresa %><br />
           <%= f.text_field :empresa %></div>

           <div><%= f.label :direccion_casa %><br />
           <%= f.text_field :direccion_casa %></div>

           <div><%= f.label :direccion_trabajo %><br />
           <%= f.text_field :direccion_trabajo %></div>

           <div><%= f.label :comunidad %><br />
           <%= f.text_field :comunidad %></div>

           <div><%= f.label :pais %><br />
           <%= f.text_field :pais %></div>
     </div>

     <div class="col-md-4">
       <p>Acceso</p>
            <div><%= f.label :email %><br />
            <%= f.email_field :email %></div>

            <div><%= f.label :password %><br />
            <%= f.password_field :password %></div>

            <div><%= f.label :password_confirmation %><br />
            <%= f.password_field :password_confirmation %></div>
     </div>
   </div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "devise/shared/links" %>

</div>